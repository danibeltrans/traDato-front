{
  "name": "truedata-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.5.0",
    "cross-env": "^5.1.4",
    "history": "^4.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-segment": "^1.6.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.2.6",
    "@material-ui/icons": "1.0.0-beta.42",
    "chartist": "0.10.1",
    "material-ui": "1.0.0-beta.41",
    "moment": "2.22.1",
    "node-sass-chokidar": "1.2.2",
    "npm-run-all": "4.1.2",
    "perfect-scrollbar": "1.3.0",
    "react-big-calendar": "0.18.0",
    "react-bootstrap-sweetalert": "4.2.3",
    "react-chartist": "0.13.1",
    "react-datetime": "2.14.0",
    "react-google-maps": "9.4.5",
    "react-jvectormap": "0.0.2",
    "react-nouislider": "2.0.1",
    "react-swipeable-views": "0.12.13",
    "react-table": "6.8.0",
    "react-tagsinput": "3.19.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "cross-env NODE_PATH=./src npm-run-all -p watch-css start-js",
    "build": "cross-env NODE_PATH=./src npm run build-css && react-scripts build",
    "test": "cross-env NODE_PATH=./src react-scripts test --env=jsdom",
    "eject": "cross-env NODE_PATH=./src react-scripts eject",
    "deploy": "cross-env NODE_PATH=./src npm run build"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
